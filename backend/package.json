    "express": "^4.18.2",
    "glob": "^11.0.0",
    "jsonrepair": "^3.13.2",
    "minimatch": "^10.0.1",
    "oxc-parser": "^0.115.0",
    "remeda": "^2.14.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
